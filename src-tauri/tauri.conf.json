{
  "package": {
    "productName": "PeCa Starter",
    "version": "6.0.0"
  },
  "build": {
    "distDir": "../dist",
    "devPath": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "PeCa Starter",
      "icon": ["icons/icon.ico"],
      "resources": ["yp/"],
      "externalBin": [],
      "copyright": "Progre",
      "category": null,
      "shortDescription": "",
      "longDescription": "",
      "deb": {
        "depends": [],
        "useBootstrapper": false
      },
      "macOS": {
        "frameworks": [],
        "minimumSystemVersion": "",
        "useBootstrapper": false,
        "exceptionDomain": "",
        "signingIdentity": null,
        "providerShortName": null,
        "entitlements": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "wix": {
          "language": "jp-JP",
          "license": "../LICENSE"
        }
      }
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "notification": {
        "all": true
      },
      "shell": {
        "open": true
      }
    },
    "windows": [
      {
        "title": "PeCa Starter",
        "width": 512,
        "height": 320,
        "minWidth": 320,
        "minHeight": 240,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}
