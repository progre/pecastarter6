{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "PeCa Starter",
  "version": "6.12.2",
  "identifier": "net.prgrssv.pecastarter",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "PeCa Starter",
        "width": 622,
        "height": 440,
        "minWidth": 320,
        "minHeight": 240,
        "resizable": true,
        "fullscreen": false,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/icon.ico"],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "",
      "wix": {
        "language": "ja-JP"
      }
    },
    "resources": ["yp/"],
    "externalBin": [],
    "copyright": "Progre",
    "category": null,
    "shortDescription": "",
    "longDescription": "",
    "macOS": {
      "frameworks": [],
      "entitlements": null,
      "minimumSystemVersion": "",
      "exceptionDomain": "",
      "signingIdentity": null,
      "providerShortName": null
    },
    "linux": {
      "deb": {
        "depends": []
      }
    },
    "licenseFile": "../LICENSE"
  },
  "mainBinaryName": "PeCa Starter"
}
